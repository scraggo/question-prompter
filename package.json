{
  "name": "node-question-prompter",
  "version": "0.0.1",
  "description": "prompt for user configured questions and notes",
  "main": "src/index.js",
  "author": "scraggo@gmail.com",
  "scripts": {
    "build": "npm run clean && babel src --out-dir build",
    "clean": "rm -rf build",
    "inspect": "npm run start-watch -- --inspect",
    "lint": "eslint . --ext .js",
    "start": "node --inspect build/index.js  -c \"./src/example/example.json\" --outputFormat=yaml",
    "start-dev": "babel-node src/index.js",
    "start-dev-ex": "babel-node src/index.js -c \"./src/example/example.json\" --outputFormat=yaml",
    "start-watch": "nodemon --exec babel-node src/index.js",
    "test": "mocha src/*.spec.js"
  },
  "dependencies": {
    "commander": "^4.0.0",
    "date-fns": "^2.15.0",
    "inquirer": "^7.3.3",
    "js-yaml": "^3.14.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@types/chai": "^4.2.22",
    "@types/js-yaml": "^3.12.7",
    "@types/mocha": "^9.0.0",
    "@types/sinon": "^10.0.6",
    "babel-eslint": "^10.0.1",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^2.3.0",
    "mocha": "^9.1.3",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.1",
    "prettier-eslint": "^8.8.2",
    "sinon": "^12.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run build",
      "pre-push": "npm run lint"
    }
  }
}
